<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg"
    tools:context=".mainactivity2">

    <Button
        android:id="@+id/mvbtn6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="108dp"
        android:layout_marginEnd="63dp"
        android:fontFamily="@font/gw"
        android:text="직접 기입"
        android:textColor="@color/basecolor"
        android:backgroundTint="@color/pastelpurple"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.816"
        app:layout_constraintStart_toEndOf="@+id/mvbtn5"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/mvbtn5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="73dp"
        android:layout_marginTop="108dp"
        android:fontFamily="@font/gw"
        android:text="스캐닝"
        android:textColor="@color/basecolor"
        android:backgroundTint="@color/pastelpurple"
        android:textColorLink="#250D0D"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/칼로리"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="190dp"
        android:fontFamily="@font/gw"
        android:text="칼로리"
        android:textColor="@color/basecolor"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/탄수화물"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="33dp"
        android:fontFamily="@font/gw"
        android:text="탄수화물"
        android:textColor="@color/basecolor"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/칼로리" />

    <EditText
        android:id="@+id/칼로리표시칸"

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="42dp"
        android:layout_marginTop="96dp"
        android:layout_marginEnd="68dp"
        android:ems="10"
        android:inputType="date"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/칼로리"
        app:layout_constraintTop_toBottomOf="@+id/title" />

    <EditText
        android:id="@+id/탄수화물표시칸"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="22dp"
        android:layout_marginTop="15dp"
        android:editable="false"
        android:ems="10"
        android:inputType="date"
        app:layout_constraintStart_toEndOf="@+id/탄수화물"
        app:layout_constraintTop_toBottomOf="@+id/칼로리표시칸" />

    <EditText
        android:id="@+id/단백질표시칸"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="41dp"
        android:layout_marginTop="15dp"
        android:ems="10"
        android:inputType="date"
        app:layout_constraintStart_toEndOf="@+id/단백질"
        app:layout_constraintTop_toBottomOf="@+id/탄수화물표시칸" />

    <EditText
        android:id="@+id/지방표시칸"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="15dp"
        android:ems="10"
        android:inputType="date"
        app:layout_constraintStart_toEndOf="@+id/지방"
        app:layout_constraintTop_toBottomOf="@+id/단백질표시칸" />

    <TextView
        android:id="@+id/단백질"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="33dp"
        android:text="단백질"
        android:textColor="@color/basecolor"
        android:fontFamily="@font/gw"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/탄수화물" />

    <TextView
        android:id="@+id/지방"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="33dp"
        android:text="지방"
        android:textColor="@color/basecolor"
        android:fontFamily="@font/gw"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/단백질" />

    <TextView
        android:id="@+id/title"
        android:layout_width="303dp"
        android:layout_height="0dp"
        android:layout_marginTop="48dp"
        android:textAlignment="center"
        android:fontFamily="@font/gw"
        android:text="영양성분 관리모드"
        android:textColor="@color/basecolor"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>

