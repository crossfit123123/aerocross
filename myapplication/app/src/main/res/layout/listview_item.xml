<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <TextView
        android:id="@+id/textcal"
        android:layout_width="45dp"
        android:layout_height="16dp"
        android:layout_marginTop="16dp"
        android:layout_weight="1"
        android:fontFamily="@font/gw"
        android:gravity="center"
        android:text="칼로리:"
        android:textAlignment="center"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/imageView1"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textcalbo"
        android:layout_width="45dp"
        android:layout_height="16dp"
        android:layout_weight="1"
        android:fontFamily="@font/gw"
        android:gravity="center"
        android:text="탄:"
        android:textAlignment="center"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/imageView1"
        app:layout_constraintTop_toBottomOf="@+id/textcal" />

    <TextView
        android:id="@+id/textcal3"
        android:layout_width="45dp"
        android:layout_height="16dp"
        android:layout_weight="1"
        android:fontFamily="@font/gw"
        android:gravity="center"
        android:text="단:"
        android:textAlignment="center"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/imageView1"
        app:layout_constraintTop_toBottomOf="@+id/textcalbo" />

    <TextView
        android:id="@+id/textcal4"
        android:layout_width="45dp"
        android:layout_height="16dp"
        android:layout_weight="1"
        android:fontFamily="@font/gw"
        android:gravity="center"
        android:text="지:"
        android:textAlignment="center"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/imageView1"
        app:layout_constraintTop_toBottomOf="@+id/textcal3" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="143dp"
        android:layout_height="80dp"
        android:layout_weight="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:fontFamily="@font/gw"
        />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="144dp"
        android:layout_height="16dp"
        android:layout_marginEnd="124dp"
        android:layout_weight="1"
        android:fontFamily="@font/gw"
        android:gravity="center"
        android:text="TextView"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvcalorie"
        android:layout_width="100dp"
        android:layout_height="16dp"
        android:layout_marginTop="16dp"
        android:layout_weight="1"
        android:fontFamily="@font/gw"
        android:gravity="center"
        android:text="TextView"
        android:textAlignment="center"
        app:layout_constraintStart_toEndOf="@+id/textcal"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvcalbo"
        android:layout_width="100dp"
        android:layout_height="16dp"
        android:layout_weight="1"
        android:fontFamily="@font/gw"
        android:gravity="center"
        android:text="TextView"
        android:textAlignment="center"
        app:layout_constraintStart_toEndOf="@+id/textcalbo"
        app:layout_constraintTop_toBottomOf="@+id/tvcalorie" />

    <TextView
        android:id="@+id/tvprotein"
        android:layout_width="100dp"
        android:layout_height="16dp"
        android:layout_weight="1"
        android:fontFamily="@font/gw"
        android:gravity="center"
        android:text="TextView"
        android:textAlignment="center"
        app:layout_constraintStart_toEndOf="@+id/textcal3"
        app:layout_constraintTop_toBottomOf="@+id/tvcalbo" />

    <TextView
        android:id="@+id/tvcount"
        android:layout_width="120dp"
        android:layout_height="49dp"
        android:layout_marginTop="32dp"
        android:layout_weight="1"
        android:fontFamily="@font/gw"
        android:gravity="center"
        android:text="TextView"
        android:textAlignment="center"
        app:layout_constraintStart_toEndOf="@+id/tvcalbo"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvfat"
        android:layout_width="100dp"
        android:layout_height="16dp"
        android:layout_weight="1"
        android:fontFamily="@font/gw"
        android:gravity="center"
        android:text="TextView"
        android:textAlignment="center"
        app:layout_constraintStart_toEndOf="@+id/textcal4"
        app:layout_constraintTop_toBottomOf="@+id/tvprotein" />

</androidx.constraintlayout.widget.ConstraintLayout>